    --
    -- The elements in the users subgroup of the configuration group
    --

    userTable   OBJECT-TYPE
            SYNTAX  SEQUENCE OF UserEntry
            ACCESS  not-accessible
            STATUS  mandatory
            DESCRIPTION
                    ""
            ::= { users 1 }

    userEntry   OBJECT-TYPE
            SYNTAX  UserEntry
            ACCESS  not-accessible
            STATUS  mandatory
            DESCRIPTION
                    ""
            INDEX   { userId }
            ::= { userTable 1 }

    UserEntry   ::=
            SEQUENCE 
            {
                userId                      INTEGER,
                userLoginId                 DisplayString,
                userPassword                DisplayString,
                userSecLevel                INTEGER,
                userIdleTimeoutEnabled      INTEGER,
                userIdleTimeout             INTEGER,
                userPwdAging                INTEGER,
                userStatus                  INTEGER
            }

    userId    OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "User Id."
        ::= { userEntry 1 }

    userLoginId    OBJECT-TYPE
        SYNTAX DisplayString
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "User login Id."
        ::= { userEntry 2 }

    userPassword    OBJECT-TYPE
        SYNTAX DisplayString
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "User password."
        ::= { userEntry 3 }

    userSecLevel    OBJECT-TYPE
        SYNTAX INTEGER  { read-only(1), read-write(2), admin(3) }
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "User security level."
        ::= { userEntry 4 }

    userIdleTimeoutEnabled    OBJECT-TYPE
        SYNTAX INTEGER  { yes(1), no(2) }
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Enables/disabled user session idle timer."
        ::= { userEntry 5 }

    userIdleTimeout    OBJECT-TYPE
        SYNTAX INTEGER  (5..30)
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "User session idle timeout."
        ::= { userEntry 6 }

    userPwdAging    OBJECT-TYPE
        SYNTAX INTEGER  { enabled(1), disabled(2) }
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Enables/disables password aging.  If password aging is
             enabled the user account will expire and be disabled
             after 45 days."
        ::= { userEntry 7 }

    userStatus    OBJECT-TYPE
        SYNTAX INTEGER  { active(1), inactive(2) }
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
            "Show whether the user is logged in active) or not (inactive)."
        ::= { userEntry 8 }

