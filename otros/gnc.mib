    --
    -- The elements in the eqpt subgroup of the configuration group
    --

    eqptTable   OBJECT-TYPE
            SYNTAX  SEQUENCE OF EqptEntry
            ACCESS  not-accessible
            STATUS  mandatory
            DESCRIPTION
                    "Summary table of modules known by the GNC."
            ::= { eqpt 1 }

    eqptEntry   OBJECT-TYPE
            SYNTAX  EqptEntry
            ACCESS  not-accessible
            STATUS  mandatory
            DESCRIPTION
                    "Equipment entry format"
            INDEX   { eqptBay, eqptShelf, eqptSlot }
            ::= { eqptTable 1 }

    EqptEntry   ::=
            SEQUENCE 
            {
                eqptBay                     INTEGER,
                eqptShelf                   INTEGER,
                eqptSlot                    INTEGER,
                eqptCardType                INTEGER,
                eqptItuNum                  ItuSelection,
                eqptSubType                 DisplayString,
                eqptSlipSpeed               INTEGER,
                eqptPrimaryState            INTEGER,
                eqptSecondaryState          INTEGER
            }

