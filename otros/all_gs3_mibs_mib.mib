    SEQUENCE {

    devDs3CurrentEB
            PerfCurrentCount,
    devDs3CurrentES
            PerfCurrentCount,
    devDs3CurrentSES
            PerfCurrentCount,
    devDs3CurrentPlcpSEFS
            PerfCurrentCount

    }


