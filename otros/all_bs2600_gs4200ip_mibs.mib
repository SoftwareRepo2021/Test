    MODULE   -- this module
        MANDATORY-GROUPS { 
            pdnReachDSLGeneralConfigGroup,
            pdnReachDSLGeneralInformationGroup
        }

-- The groups :
--      pdnReachDSLEWLModeGroup
--      pdnReachDSLLoopLengthModeGroup
