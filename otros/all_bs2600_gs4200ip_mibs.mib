-- =========================================================================
-- ==                  MIB Objects                                        ==
-- =========================================================================


devSecurityMgrValidation OBJECT-TYPE
    SYNTAX      INTEGER {
                    disable (1),
                    enable  (2)
                }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION "This object is used to either enable or disable
                 IP validation.  When enabled, the first check on any
                 incoming PDU is based on the IP address of the packet.
                 If the IP address is configured in one of the manager 
                 tables, access is granted according to the level of 
                 access configured for that IpAddress.

                 If the implementation is such that there are separate
                 objects used for configuring the validation of IP 
                 addresses for Web Clients (via 
                 devSecurityWebSourceValidation) and Telnet Clients 
                 (via devSecurityTelnetSourceValidation), this object 
                 will be used to configure the validation of IP 
                 addresses for SNMP managers.  Otherwise the 
                 implementation is such that there is one object, 
                 namely this object, that configures the validation of
                 IP addresses for all clients and managers (SNMP, Web, 
                 and Telnet)."
    ::= { pdn-security 1 }


devSecurityMgrMaxNumber OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Maximum number of managers allowed in the 
                 devSecurityMgrTable."
    ::= { pdn-security 2 }


devSecurityMgrCurrentNumber OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Current number of managers in the devSecurityMgrTable."
    ::= { pdn-security 3 }


-- =========================================================================
-- == devSecurityMgrTable (Deprecated)
-- =========================================================================

devSecurityMgrTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF DevSecurityMgrEntry
    MAX-ACCESS  not-accessible
    STATUS      deprecated
    DESCRIPTION "A list of the security manager entries.
                 In order to add a new Security Manager the 
                 devSecurityMgrIpAddress object must be created first by 
                 setting it to a valid IP Address."
    ::= { pdn-security 4 }

devSecurityMgrEntry OBJECT-TYPE
    SYNTAX      DevSecurityMgrEntry
    MAX-ACCESS  not-accessible
    STATUS      deprecated
    DESCRIPTION "The SNMP IP Address configuration items."
    INDEX       { devSecurityMgrIpAddress }
    ::= { devSecurityMgrTable 1 }

DevSecurityMgrEntry ::=
    SEQUENCE {
        devSecurityMgrIpAddress  IpAddress,
        devSecurityMgrAccess     INTEGER
    }

devSecurityMgrIpAddress OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-write
    STATUS      deprecated
    DESCRIPTION "This is used to specify the IP address that identifies the
                 SNMP manager(s) that are authorized to send SNMP messages.

                 An existing Security Manager may be deleted by setting the 
                 devSecurityMgrIpAddress object to 0.0.0.0 or by setting the 
                 devSecurityMgrIpAddress object to a new IP Address.
                 A new Security Manager may be added by setting
                 the devSecurityMgrIpAddress object of 0.0.0.0 to the new 
                 address as long as there is still room for another Security 
                 Manager.

                 When ever a new Security Manager is added or an existing 
                 Security Manager's IP Address is modified, 
                 the devSecurityMgrAccess object is set to the default value 
                 of read (1)."
    ::= { devSecurityMgrEntry 1 }

devSecurityMgrAccess OBJECT-TYPE
                SYNTAX INTEGER {
                    read      (1),
                    readWrite (2)
                }
    MAX-ACCESS  read-write
    STATUS      deprecated
    DESCRIPTION "This field is used to specify the access allowed for an 
                 authorized NMS when Manager Validation is performed. 
                 If the IP address for the NMS sending an SNMP message is on 
                 the list of allowed managers, this option along with the 
                 Community Name access determine the type of access allowed 
                 for the that manager. 

                 When ever a new Security Manager is added or an existing 
                 Security Manager's IP Address is modified, the 
                 devSecurityMgrAccess object is set to the default value of 
                 read (1).  If the index for this object is not in the 
                 existing table and if there is room in the table then the 
                 index will be used to create a new Security Manager and 
                 will be given the requested access level."
    ::= { devSecurityMgrEntry 2 }


-- =========================================================================
-- == newSecurityMgrTable (Deprecated)                                    ==
-- =========================================================================

newSecurityMgrTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF NewSecurityMgrEntry
    MAX-ACCESS  not-accessible
    STATUS      deprecated
    DESCRIPTION "A list of the security manager entries.  If an entry is not 
                 found in this table, it is added to the table."
    ::= { pdn-security 5 }

newSecurityMgrEntry OBJECT-TYPE
    SYNTAX      NewSecurityMgrEntry
    MAX-ACCESS  not-accessible
    STATUS      deprecated
    DESCRIPTION "The SNMP IP Address configuration items."
    INDEX       { newSecurityMgrIpAddress }
    ::= { newSecurityMgrTable 1 }

NewSecurityMgrEntry ::=
    SEQUENCE {
        newSecurityMgrIpAddress    IpAddress,
        newSecurityMgrAccess       INTEGER,
        newSecurityMgrSubnetMask   IpAddress
    }

newSecurityMgrIpAddress OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-write
    STATUS      deprecated
    DESCRIPTION "This is used to specify the IP address that identifies the
                 SNMP manager(s) that are authorized to send SNMP messages.
                 This object is the binary AND of SNMP or telnet source IP
                 address and its subnet mask if the subnet mask is 
                 available.  Subnet mask is assumed to be 255.255.255.255 
                 when not available"
    ::= { newSecurityMgrEntry 1 }


newSecurityMgrAccess OBJECT-TYPE
    SYNTAX      INTEGER {
                    read      (1),
                    readWrite (2),
                    noAccess  (3),
                    telnetNoAccess  (4),
                    telnetRead      (5),
                    telnetReadWrite (6)
                }
    MAX-ACCESS  read-write
    STATUS      deprecated
    DESCRIPTION "This field is used to specify the access allowed for an 
                 authorized NMS when Manager Validation is performed.  If 
                 the IP address for the NMS sending an SNMP message is on 
                 the list of allowed managers, this option along with the 
                 Community Name access  determine the type of access allowed
                 for the  that manager. 

                 When ever a new Security Manager is added or an existing 
                 Security Manager's IP Address is modified, the 
                 newSecurityMgrAccess object is set to the default value of 
                 read (1).  If the index for this object is not in the 
                 existing table and if there is room in the table then the 
                 index will be used to create a new Security Manager and 
                 will be given the requested access level.  In addition 
                 changing an existing access permission to noAccess 
                 invalidates the corresponding row of this table.  It is up
                 to the implementation to decide whether to actually remove
                  this entry from the table or not.

                 read (1)            ==> no telnet access and snmp read only 
                                         access 
                 readWrite (2)       ==> no telnet access and snmp 
                                         read-write access
                 noAccess (3)        ==> no telnet access and no snmp 
                                         read-write access
                 telnetNoAccess (4)  ==> telnet access and no snmp access
                 telnetRead (5)      ==> telnet access and snmp read only 
                                         access
                 telnetReadWrite (6) ==> telnet access and snmp read-write 
                                         access."
    ::= { newSecurityMgrEntry 2 }

newSecurityMgrSubnetMask OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-write
    STATUS      deprecated
    DESCRIPTION "This object is used to specify the subnet mask "
    ::= { newSecurityMgrEntry 3 }



devSecurityTelnetSourceValidation OBJECT-TYPE
    SYNTAX      INTEGER {
                    disable (1),
                    enable  (2)
                }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION "This object is used to either enable or disable telnet 
                 source address screening "
    ::= { pdn-security 6 }

devSecurityFtpSourceValidation OBJECT-TYPE
    SYNTAX      INTEGER {
                    disable (1),
                    enable  (2)
                }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION "This object is used to either enable or disable ftp 
                 source address screening "
    ::= { pdn-security 7 }


-- =========================================================================
-- == securityMgrTable                                                    ==
-- =========================================================================
securityMgrTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF SecurityMgrEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "This table forms a Management Domain Access List.  
                 Each entry identifies an entity or range of entities 
                 from an IP address and a subnet mask pair.  In 
                 addition an entry defines subsections of the 
                 Management Domain that the entity or entities are
                 allowed to access.  In order for this table to be
                 used, the respective IP Validation object must be
                 enabled.  For example:

                 +---------------+-----------------------------------+
                 | Entity        | IP Validation Object              |
                 +---------------+-----------------------------------+
                 +---------------+-----------------------------------+
                 | FTP Client    | devSecurityFtpSourceValidation    |
                 +---------------+-----------------------------------+
                 | SNMP Manager  | devSecurityMgrValidation          |
                 +---------------+-----------------------------------+
                 | Web Client    | devSecurityWebSourceValidation    |
                 +---------------+-----------------------------------+
                 | Telnet Client | devSecurityTelnetSourceValidation |
                 +---------------+-----------------------------------+

                 This table is also used to specify SNMP 
                 Trap/Notification managers which traps/notifications
                 are to be set to if the entry has its 
                 securityMgrTrapAccess object set to trapAccess(1)."
    ::= { pdn-security 8 }

securityMgrEntry OBJECT-TYPE
    SYNTAX      SecurityMgrEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "An entry consists of both an IP address and subnet 
                 mask pair to uniquely identify an entity or range
                 of entities whose access to the Management Domain
                 are defined."
    INDEX       { securityMgrIpAddress, securityMgrSubnetMask}
    ::= { securityMgrTable 1 }

SecurityMgrEntry ::=
    SEQUENCE {
        securityMgrIpAddress      IpAddress,
        securityMgrSubnetMask     IpAddress,
        securityMgrSnmpAccess     INTEGER,
        securityMgrTelnetAccess   INTEGER,
        securityMgrFtpAccess      INTEGER,
        securityMgrTrapAccess     INTEGER,
        securityMgrRowStatus      RowStatus,
        securityMgrWebAccess      INTEGER
    }

securityMgrIpAddress OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Specifies the IP address portion of an entry."
    ::= { securityMgrEntry 1 }

securityMgrSubnetMask OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Specifies the subnet mask portion of an entry."
    ::= { securityMgrEntry 2 }

securityMgrSnmpAccess OBJECT-TYPE
    SYNTAX      INTEGER {
                    readOnly  (1),
                    readWrite (2),
                    noAccess (3)
                }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION "This is used to specify the snmp access to the device."
    ::= { securityMgrEntry 3 }

securityMgrTelnetAccess OBJECT-TYPE
    SYNTAX      INTEGER {
                    disable (1),
                    enable  (2)
                }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION "This is used to specify the telnet access to the device."
    ::= { securityMgrEntry 4 }

securityMgrFtpAccess OBJECT-TYPE
    SYNTAX      INTEGER {
                    disable (1),
                    enable  (2)
    }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION "This is used to specify the ftp access to the device."
    ::= { securityMgrEntry 5 }

securityMgrTrapAccess OBJECT-TYPE
    SYNTAX      INTEGER {
                    trapAccess  (1),
                    noTrapAccess (2)
                }
    MAX-ACCESS  read-write
    STATUS      deprecated
    DESCRIPTION "This is used to specify the trap access to the device.
                 NOTE: the pdnDevTrapMgrTable should be used instead."
    ::= { securityMgrEntry 6 }

securityMgrRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION "The status of this table entry."
    ::= { securityMgrEntry 7 }

securityMgrWebAccess OBJECT-TYPE
    SYNTAX      INTEGER {
                    disable (1),
                    enable  (2)
                }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION "This is used to specify the Web Client access to the 
                 device."
    ::= { securityMgrEntry 8 }



devSecuritySNMPMgrAccess OBJECT-TYPE
    SYNTAX      INTEGER {
                    disable (1),
                    enable  (2)
                }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION "This object is used to either enable or disable
                 SNMP access to the system.  When disabled the first check 
                 on any incoming PDU is based on the PDU type of the 
                 datagram.  If it is an SNMP manager trying to access SNMP 
                 agent on the system the access will be denied."
    ::= { pdn-security 9 }

devSecurityMgrFailedValidationCount OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "When IP Validation is enabled 
                 [devSecurityMgrValidation is set to enable(2)], this
                 object counts the number of IP packets that have failed
                 the IP Validation check.  Otherwise this object should
                 return 0.

                 If the implementation is such that there are separate
                 objects used for configuring the validation of IP 
                 addresses for Web Clients (via 
                 devSecurityWebSourceValidation) and Telnet Clients 
                 (via devSecurityTelnetSourceValidation), this object
                 will be used to count the number of IP packets that 
                 have failed the IP validation check for SNMP managers.
                 Otherwise the implementation is such that there is one
                 object, namely this object, that count the number of
                 IP packets that have failed the IP validation check 
                 for all clients and managers (SNMP, Web, and Telnet)."
    ::= {  pdn-security 10 }

devSecurityWebServerAccess OBJECT-TYPE
    SYNTAX      INTEGER {
                    disable (1),
                    enable  (2)
                }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION "This object is used to either enable or disable access to 
                 the Web Server."
    ::= { pdn-security 11 }

devSecuritySNMPTrapRateLimitPeriod OBJECT-TYPE
    SYNTAX      Integer32(0..2147483647)
    UNITS       "seconds"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION "The SNMP Trap Rate Limiting Period.  This period is the
                 gap, in seconds, that the agent enforces, between
                 consecutive traps/notifications for the same instance
                 of a particular event.  Events that occur before this 
                 period expires will not be sent to a manager.  Setting 
                 this object to 0, disables rate limiting.  

                 Care must be taken when assigning a value to this object.
                 Disabling rate limiting for traps/Notifications could
                 result in a large amount of traps/notifications being
                 sent to a manager.  Assigning a large value could prevent
                 the timely delivery of important diagnostic information."
    ::= { pdn-security 12 }

devSecurityWebSourceValidation OBJECT-TYPE
    SYNTAX      INTEGER {
                    disable (1),
                    enable  (2)
                }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION "This object is used to enable or disable IP validation
                 for Web Client access to the device's Web Server.  

                 When enabled, IP validation for Web Clients is enabled.
                 A Web Client `access control list' consists of entries
                 in the securityMgrTable which are associated with Web 
                 access (securityMgrWebAccess is set to `enable').  The
                 IP validation occurs such that the source IP address 
                 in a HTTP PDU from a Web Client must exist in the Web 
                 Client `access control list' in order for the PDU to 
                 be processed by the device's Web Server.  If the IP
                 address is not in the `access control list', the PDU
                 is discarded.

                 When disabled, the source IP address in the HTTP PDU is
                 not validated."
    ::= { pdn-security 13 }

devSecurityWebFailedValidationCount OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "When devSecurityWebSourceValidation is enabled, this 
                 object counts the number of HTTP PDUs discarded because
                 their source IP address was not in the Web Client 
                 `access control list'.  When 
                 devSecurityWebSourceValidation is disabled, this 
                 object should return a 0."
    ::= {  pdn-security 14 }

devSecurityTelnetServerAccess OBJECT-TYPE
    SYNTAX      INTEGER {
                    disable (1),
                    enable  (2)
                }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION "This object is used to enable or disable access to the
                 Telnet Server."
    ::= { pdn-security 15 }

devSecurityTelnetFailedValidationCount OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "When devSecurityTelnetSourceValidation is enabled, 
                 this object counts the number of Telnet PDUs discarded
                 because their source IP address was not in the Telnet 
                 Client `access control list'.  When 
                 devSecurityTelnetSourceValidataion is disabled, this 
                 object should return a 0."
    ::= {  pdn-security 16 }

devSecurityNotificationsEnable OBJECT-TYPE
    SYNTAX      BITS {
                    devSecurityWebNotification(0),
                    devSecurityTelnetNotification(1)
                }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION "Provides the ability to enable or disable notifications
                 for this MIB module.  When the bit is set for a 
                 particular notification, the notification must be 
                 generated unless it is to be rate limited.  Reference 
                 `NOTES' for Traps/Notifications below.  When the bit is
                 reset for a particular notification, the notification 
                 must not be generated."
    ::= { pdn-security 17 }


-- =====================================================================
-- == devSecurityAuthenFailedTable                                    ==
-- =====================================================================
devSecurityAuthenFailedTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF DevSecurityAuthenFailedEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "This table contains a list of IP addresses that have
                 attempted to access the SNMP Agent, Web Server, or the
                 Telnet Server and were not considered to be `invalid' 
                 because they were not in the respective `access control 
                 list'.  The maximum number of entries in the table is 
                 implementation dependent."
    ::= { pdn-security 18 }

devSecurityAuthenFailedEntry OBJECT-TYPE
    SYNTAX      DevSecurityAuthenFailedEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "An entry consists primarily of the IP address that was
                 considered `invalid' along with the time stamp of when
                 it was observed and which `access control list' 
                 considered it `invalid'."
    INDEX       { devSecurityAuthenFailedIndex }
    ::= { devSecurityAuthenFailedTable 1 }

DevSecurityAuthenFailedEntry ::=
    SEQUENCE {
        devSecurityAuthenFailedIndex                  Unsigned32,
        devSecurityAuthenFailedRowStatus              RowStatus,
        devSecurityAuthenFailedDateAndTime            DateAndTime,
        devSecurityAuthenFailedIpAddress              IpAddress,
        devSecurityAuthenFailedList                   INTEGER
    }

devSecurityAuthenFailedIndex OBJECT-TYPE
    SYNTAX      Unsigned32(1..4294967296)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "Identifies a unique entry in the table.  Lower 
                 numerical values indicate older entries in the table.
                 It is acceptable to reset the value of this object if
                 entries are aged out or manually removed and the table
                 becomes empty.  The maximum number of entries in the 
                 table is implementation specific."
    ::= { devSecurityAuthenFailedEntry 1 }

devSecurityAuthenFailedRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION "This object exists only to allow a manager to
                 remove entries in the table.  As a result, only
                 active(1) and destroy(6) are supported."
    ::= { devSecurityAuthenFailedEntry 2 }

devSecurityAuthenFailedDateAndTime OBJECT-TYPE
    SYNTAX      DateAndTime
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Provides a date-time indication of when the
                 attempt to access the SNMP Agent with an
                 `invalid' IP address occurred."
    ::= { devSecurityAuthenFailedEntry 3 }

devSecurityAuthenFailedIpAddress OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The IP address that was considered `invalid'."
    ::= { devSecurityAuthenFailedEntry 4 }

devSecurityAuthenFailedList OBJECT-TYPE
    SYNTAX      INTEGER {
                    snmp(1),
                    web(2),
                    telnet(3)
                }
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Identifies the `access control list' the IP address
                 was not in which resulted in it being considered
                 `invalid'."
    ::= { devSecurityAuthenFailedEntry 5 }



devSecurityAuthenFailedAgeOutTime OBJECT-TYPE
    SYNTAX      Integer32(0..2147483647)
    UNITS       "seconds"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION "The time, in seconds, to automatically age out entries
                 in the devSecurityAuthenFailedTable.  Setting this 
                 object to 0, disables the aging out of entries in the
                 table."
    ::= { pdn-security 19 }



-- =====================================================================
-- ==                                                                 ==
-- ==  { pdn-security 99 } is reserved for pdnSecurityAFNs            ==
-- ==  which is the Accessible for Notifications group                ==
-- ==                                                                 ==
-- =====================================================================



-- =========================================================================
-- ==       Notification objects (Accessible for Notifications)           ==
-- =========================================================================

devSecurityFailedIpAddress OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION "The IP address of a client attempting to access the
                 device which has failed validation because it is not
                 in an entities `access control list'."
    ::= { pdnSecurityAFNs 1 }



-- =========================================================================
-- ==                    Traps/Notifications                              ==
-- =========================================================================

--
-- NOTES: 
--    1.  SNMP uses the authenticationFailure notification for an `IP 
--        Address Validation Event.'  Also Paradyne uses the 
--        devAuthenticationFailureIpAddress object to communicate the 
--        address that failed validation.
--    2.  Notifications should be rate limited (Throttled) such that there
--        is at least a one-minute gap between the generation of 
--        consecutive notifications of the same event.  When notifications
--        are rate limited, they are dropped and not queued for sending
--        at a future time.

devSecurityWebAuthenFailure NOTIFICATION-TYPE
    OBJECTS {
        devSecurityFailedIpAddress
    }
    STATUS      current
    DESCRIPTION "An IP address for a Web Client is not in the `access
                 control list'."
    ::= { pdnSecurityNotifications 1 } 

devSecurityTelnetAuthenFailure NOTIFICATION-TYPE
    OBJECTS {
        devSecurityFailedIpAddress    
    }
    STATUS      current
    DESCRIPTION "An IP address for a Telnet Client is not in the 
                 `access control list'."
    ::= { pdnSecurityNotifications 2 } 


