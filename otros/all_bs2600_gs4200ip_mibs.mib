     atmMIB OBJECT IDENTIFIER ::= { mib-2 37 }
     
     atmMIBObjects  OBJECT IDENTIFIER ::= {atmMIB 1}
     
     atmTCMIB MODULE-IDENTITY
          LAST-UPDATED "9810190200Z"
          ORGANIZATION "IETF AToMMIB Working Group"
          CONTACT-INFO
            "          Michael Noto
