    ::= { pdnAdslLineSubCarStatusEntry 8 }

pdnAdslLineSubCarAturSnrPs OBJECT-TYPE
    SYNTAX      Unsigned32(0..254 | 255 )
    UNITS       "dB"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Upstream Signal-to-Noise Ratio.  The special value 255
